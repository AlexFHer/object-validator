{
  "name": "@ts-lab/object-validator",
  "author": "Alex Fandi√±o <afandino99@gmail.com>",
  "version": "1.2.1",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:dev": "ng build object-validator",
    "build:production": "npm run test:no-watch && npm run lint && ng build object-validator",
    "test": "ng test object-validator",
    "test:no-watch": "ng test object-validator --watch=false --code-coverage",
    "test:coverage": "ng test object-validator --code-coverage",
    "lint": "ng lint object-validator"
  },
  "license": "MIT",
  "typings": "index.d.ts",
  "description": "A very lightweight library to create reusable validation configurations to validate objects in typescript.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "object validator",
    "typescript object validator",
    "object-validator",
    "typescript",
    "object",
    "validations",
    "valid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexFHer/object-validator"
  },
  "bugs": {
    "url": "https://github.com/AlexFHer/object-validator/issues"
  }
}

